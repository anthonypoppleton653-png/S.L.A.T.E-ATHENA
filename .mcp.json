{
  "mcpServers": {
    "slate": {
      "command": "powershell",
      "args": ["-ExecutionPolicy", "Bypass", "-File", "${CLAUDE_PLUGIN_ROOT}/slate/mcp_launcher.ps1"],
      "env": {
        "SLATE_PLUGIN_ROOT": "${CLAUDE_PLUGIN_ROOT}",
        "SLATE_BEHAVIOR": "operator",
        "SLATE_ACTIONGUARD": "enabled"
      },
      "description": "SLATE MCP Server - AI orchestration, workflow management, GPU, K8s deployment"
    }
  }
}
