    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": { "foreground": "#4A5A6A", "fontStyle": "italic" }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": { "foreground": "#E0C857" }
        },
        {
            "name": "Constants",
            "scope": ["constant", "constant.language"],
            "settings": { "foreground": "#B8A878" }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Control Keywords",
            "scope": ["keyword.control"],
            "settings": { "foreground": "#FF6B1A", "fontStyle": "italic" }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": { "foreground": "#8FA89C" }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": { "foreground": "#E0D8B8" }
        },
        {
            "name": "Function Parameters",
            "scope": ["variable.parameter"],
            "settings": { "foreground": "#C0B8A8" }
        },
        {
            "name": "Classes & Types",
            "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
            "settings": { "foreground": "#D4C8A8" }
        },
        {
            "name": "Interfaces",
            "scope": ["entity.name.type.interface"],
            "settings": { "foreground": "#C0B898" }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": { "foreground": "#E8D8C8" }
        },
        {
            "name": "Properties",
            "scope": ["variable.other.property", "support.variable.property"],
            "settings": { "foreground": "#B0A898" }
        },
        {
            "name": "Decorators / Annotations",
            "scope": ["meta.decorator", "punctuation.decorator"],
            "settings": { "foreground": "#D4AF37", "fontStyle": "italic" }
        },
        {
            "name": "Tags (HTML/XML)",
            "scope": ["entity.name.tag"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Tag Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": { "foreground": "#A8A08A", "fontStyle": "italic" }
        },
        {
            "name": "CSS Properties",
            "scope": ["support.type.property-name.css"],
            "settings": { "foreground": "#B0A898" }
        },
        {
            "name": "CSS Values",
            "scope": ["support.constant.property-value.css"],
            "settings": { "foreground": "#D4C8A8" }
        },
        {
            "name": "Regex",
            "scope": ["string.regexp"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Markdown Heading",
            "scope": ["heading.1.markdown", "heading.2.markdown", "heading.3.markdown", "markup.heading"],
            "settings": { "foreground": "#D4AF37", "fontStyle": "bold" }
        },
        {
            "name": "Markdown Bold",
            "scope": ["markup.bold"],
            "settings": { "foreground": "#F5F0EB", "fontStyle": "bold" }
        },
        {
            "name": "Markdown Italic",
            "scope": ["markup.italic"],
            "settings": { "foreground": "#D6D0C8", "fontStyle": "italic" }
        },
        {
            "name": "Markdown Code",
            "scope": ["markup.inline.raw"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Markdown Link",
            "scope": ["markup.underline.link"],
            "settings": { "foreground": "#4A7A9A" }
        },
        {
            "name": "JSON Keys",
            "scope": ["support.type.property-name.json"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "YAML Keys",
            "scope": ["entity.name.tag.yaml"],
            "settings": { "foreground": "#D4AF37" }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation"],
            "settings": { "foreground": "#6A7A8A" }
        },
        {
            "name": "Invalid / Deprecated",
            "scope": ["invalid", "invalid.deprecated"],
            "settings": { "foreground": "#E85050" }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "function": "#E0D8B8",
        "function.declaration": "#E0D8B8",
        "function.defaultLibrary": "#D4C8A8",
        "variable": "#E8D8C8",
        "variable.readonly": "#C8B8A8",
        "parameter": "#C0B8A8",
        "property": "#B0A898",
        "property.readonly": "#A8A088",
        "class": "#D4C8A8",
        "interface": "#C0B898",
        "enum": "#D4AF37",
        "enumMember": "#B8A878",
        "type": "#D4C8A8",
        "namespace": "#B0A898",
        "decorator": "#D4AF37",
        "string": "#D4AF37",
        "number": "#E0C857",
        "keyword": "#D4AF37",
        "comment": "#4A5A6A",
        "operator": "#8FA89C",
        "regexp": "#D4AF37"
    }
