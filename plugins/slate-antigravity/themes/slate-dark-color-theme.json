    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": { "foreground": "#525252", "fontStyle": "italic" }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted"],
            "settings": { "foreground": "#86efac" }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": { "foreground": "#fbbf24" }
        },
        {
            "name": "Constants",
            "scope": ["constant", "constant.language"],
            "settings": { "foreground": "#f59e0b" }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": { "foreground": "#4ade80" }
        },
        {
            "name": "Control Keywords",
            "scope": ["keyword.control"],
            "settings": { "foreground": "#f59e0b", "fontStyle": "italic" }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": { "foreground": "#a3a3a3" }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": { "foreground": "#e2e8f0" }
        },
        {
            "name": "Function Parameters",
            "scope": ["variable.parameter"],
            "settings": { "foreground": "#CAC4BF" }
        },
        {
            "name": "Classes & Types",
            "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
            "settings": { "foreground": "#93c5fd" }
        },
        {
            "name": "Interfaces",
            "scope": ["entity.name.type.interface"],
            "settings": { "foreground": "#7dd3fc" }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": { "foreground": "#E8E2DE" }
        },
        {
            "name": "Properties",
            "scope": ["variable.other.property", "support.variable.property"],
            "settings": { "foreground": "#a3a3a3" }
        },
        {
            "name": "Decorators / Annotations",
            "scope": ["meta.decorator", "punctuation.decorator"],
            "settings": { "foreground": "#4ade80", "fontStyle": "italic" }
        },
        {
            "name": "Tags (HTML/XML)",
            "scope": ["entity.name.tag"],
            "settings": { "foreground": "#22c55e" }
        },
        {
            "name": "Tag Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": { "foreground": "#86efac", "fontStyle": "italic" }
        },
        {
            "name": "CSS Properties",
            "scope": ["support.type.property-name.css"],
            "settings": { "foreground": "#a3a3a3" }
        },
        {
            "name": "CSS Values",
            "scope": ["support.constant.property-value.css"],
            "settings": { "foreground": "#93c5fd" }
        },
        {
            "name": "Regex",
            "scope": ["string.regexp"],
            "settings": { "foreground": "#facc15" }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": { "foreground": "#f59e0b" }
        },
        {
            "name": "Markdown Heading",
            "scope": ["heading.1.markdown", "heading.2.markdown", "heading.3.markdown", "markup.heading"],
            "settings": { "foreground": "#4ade80", "fontStyle": "bold" }
        },
        {
            "name": "Markdown Bold",
            "scope": ["markup.bold"],
            "settings": { "foreground": "#ffffff", "fontStyle": "bold" }
        },
        {
            "name": "Markdown Italic",
            "scope": ["markup.italic"],
            "settings": { "foreground": "#E8E2DE", "fontStyle": "italic" }
        },
        {
            "name": "Markdown Code",
            "scope": ["markup.inline.raw"],
            "settings": { "foreground": "#86efac" }
        },
        {
            "name": "Markdown Link",
            "scope": ["markup.underline.link"],
            "settings": { "foreground": "#60a5fa" }
        },
        {
            "name": "JSON Keys",
            "scope": ["support.type.property-name.json"],
            "settings": { "foreground": "#86efac" }
        },
        {
            "name": "YAML Keys",
            "scope": ["entity.name.tag.yaml"],
            "settings": { "foreground": "#86efac" }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation"],
            "settings": { "foreground": "#525252" }
        },
        {
            "name": "Invalid / Deprecated",
            "scope": ["invalid", "invalid.deprecated"],
            "settings": { "foreground": "#ef4444" }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "function": "#e2e8f0",
        "function.declaration": "#e2e8f0",
        "function.defaultLibrary": "#93c5fd",
        "variable": "#E8E2DE",
        "variable.readonly": "#CAC4BF",
        "parameter": "#CAC4BF",
        "property": "#a3a3a3",
        "property.readonly": "#737373",
        "class": "#93c5fd",
        "interface": "#7dd3fc",
        "enum": "#86efac",
        "enumMember": "#4ade80",
        "type": "#93c5fd",
        "namespace": "#22c55e",
        "decorator": "#4ade80",
        "string": "#86efac",
        "number": "#fbbf24",
        "keyword": "#4ade80",
        "comment": "#525252",
        "operator": "#a3a3a3",
        "regexp": "#facc15"
    }
