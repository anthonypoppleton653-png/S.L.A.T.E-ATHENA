{
  "servers": {
    "slate": {
      "type": "stdio",
      "command": "${workspaceFolder}/.venv/Scripts/python.exe",
      "args": ["slate/mcp_server.py"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "PYTHONIOENCODING": "utf-8",
        "CUDA_VISIBLE_DEVICES": "0,1",
        "SLATE_WORKSPACE": "${workspaceFolder}"
      }
    }
  }
}
