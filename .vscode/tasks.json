			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			}
		},
		{
			"label": "SLATE: Open Avatar",
			"type": "process",
			"command": "cmd",
			"args": ["/c", "start", "http://localhost:5000"],
			"presentation": {
				"reveal": "never",
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "SLATE: Voice Test",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
