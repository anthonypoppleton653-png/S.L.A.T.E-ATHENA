{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "SLATE: Auto-Start",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/slate_startup.py"],
			"isBackground": true,
			"problemMatcher": {
				"pattern": {
					"regexp": "^$"
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "S.L.A.T.E. Starting",
					"endsPattern": "SLATE Ready|Dashboard.*Running"
				}
			},
			"presentation": {
				"reveal": "silent",
				"panel": "dedicated",
				"showReuseMessage": false
			},
			"runOptions": {
				"runOn": "folderOpen"
			}
		},
		{
			"label": "SLATE: Start",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/slate_startup.py"],
			"isBackground": false,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "SLATE: Stop",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/slate_startup.py", "--stop"],
			"isBackground": false,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			}
		},
		{
			"label": "SLATE: Status",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/slate_startup.py", "--status"],
			"isBackground": false,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			}
		},
		{
			"label": "SLATE: Dashboard",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/agents/slate_dashboard_server.py"],
			"isBackground": true,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			}
		},
		{
			"label": "SLATE: Open Dashboard",
			"type": "process",
			"command": "cmd",
			"args": ["/c", "start", "http://localhost:8080"],
			"presentation": {
				"reveal": "never",
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "SLATE: Avatar Server",
