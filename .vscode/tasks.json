{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "SLATE: Auto-Start",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/slate_startup.py"],
			"isBackground": true,
			"problemMatcher": {
				"pattern": {
					"regexp": "^$"
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "S.L.A.T.E. Starting",
					"endsPattern": "SLATE Ready|Dashboard.*Running"
				}
			},
			"presentation": {
				"reveal": "silent",
				"panel": "dedicated",
				"showReuseMessage": false
			},
			"runOptions": {
				"runOn": "folderOpen"
			}
		},
		{
			"label": "SLATE: Start",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/slate_startup.py"],
			"isBackground": false,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "SLATE: Stop",
			"type": "shell",
