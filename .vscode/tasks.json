		},
		{
			"label": "SLATE: Agent System",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-ExecutionPolicy", "Bypass",
				"-File", "${workspaceFolder}/caduceus/agents/launch_agent_system.ps1"
			],
			"isBackground": true,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true
			},
			"group": {
				"kind": "test",
				"isDefault": true
			}
		},
		{
			"label": "SLATE: Setup Models",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/caduceus/agents/setup_local_models.py"],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			},
			"group": "build"
		},
		{
			"label": "SLATE: Ollama (Dual GPU)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-ExecutionPolicy", "Bypass",
				"-Command", "Stop-Process -Name ollama -ErrorAction SilentlyContinue; $env:OLLAMA_NUM_GPU='2'; ollama serve"
			],
			"isBackground": true,
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true
			}
		},
		{
			"label": "SLATE: Continue Workflow",
			"type": "shell",
			"command": "py",
			"args": ["slate_workflow.py", "--continue"],
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "shared",
				"focus": true,
				"clear": true
			}
		},
		{
			"label": "SLATE: Workflow Status",
			"type": "shell",
